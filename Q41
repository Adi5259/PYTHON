# WAP to print prime numbers between 1 and 100
for i in range(2, 101):  # 1 is not prime
    is_prime = True
    for j in range(2, i):
        if i % j == 0:
            is_prime = False
            break
    if is_prime:
        print(i, end=" ") # end="" print all the prime numbers in one line with white space in between them
